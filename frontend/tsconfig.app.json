{
  "compilerOptions": {
    "target": "ES2021",
    "useUnknownInCatchVariables": false,
    "lib": ["ESNext", "DOM"],
    "types": ["vite/client"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["./src"]
}
